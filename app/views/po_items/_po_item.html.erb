<div id="<%= dom_id po_item %>">
  <p>
    <strong>Bid:</strong>
    <%= po_item.bid %>
  </p>

  <p>
    <strong>Name:</strong>
      <%= po_item.name %>
  </p>

  <p>
    <strong>Price:</strong>    
    <%= po_item.price %>
  </p>

   <p>
    <strong>Quantity:</strong>
    <%= po_item.quantity %>
